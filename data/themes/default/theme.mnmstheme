		object{
			picture(%THEMES%/default/blocks/checkpoint.png,0,0,50,50)
		}
	}
	blockState(activated){
		object{
			animation=16,0
			picture(%THEMES%/default/blocks/checkpoint.png,50,0,50,50)
			offsetAnimation{
				point(0,0)
				point(0,-8,4,1)
				point(0,8,8,1)
				point(0,0,4,1)
			}
		}
	}
}
