		object{
			picture(%THEMES%/default/blocks/swap.png,0,0,50,50)
		}
	}
	blockState(activated){
		oneTimeAnimation=24,default
		object{
			animation=12,0
			pictureAnimation(%THEMES%/default/blocks/swap.png){
